<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Obsolete</fullName>
    <active>true</active>
    <errorConditionFormula>ISNEW() &amp;&amp; NOT(ISBLANK(Account__c)) &amp;&amp; TEXT(Account__r.Account_Status__c) == &apos;Obsolete&apos;</errorConditionFormula>
    <errorMessage>Visit cannot be created for Obsolete Account.</errorMessage>
</ValidationRule>
